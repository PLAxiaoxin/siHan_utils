{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": ["./src"],
    "includePattern": ".+\\.js(doc)?$"
  },
  "plugins": ["plugins/markdown"],
  "markdown": {
    "tags": ["file"],
    "excludeTags": ["author"],
    "hardwrap": true
  },
  "templates": {
    "cleverLinks": false,
    "outputSourceFiles": false,
    "outputSourcePath": false,
    "monospaceLinks": true,
    "useCollapsibles": true,
    "name": "sihan-utils"
  },
  "opts": {
    "template": "node_modules/tui-jsdoc-template",
    "destination": "./docs",
    "recurse": true
  }
}
